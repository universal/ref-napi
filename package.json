{
  "name": "@lifbi_tbt/ref-napi",
  "description": "Turn Buffer instances into \"pointers\"",
  "engines": {
    "node": ">= 10.0"
  },
  "keywords": [
    "native",
    "buffer",
    "extensions",
    "c++",
    "pointer",
    "reference",
    "dereference",
    "type",
    "int",
    "long",
    "float",
    "double",
    "byte",
    "64",
    "napi"
  ],
  "version": "3.0.4",
  "license": "MIT",
  "author": "Anna Henningsen <anna@addaleax.net>",
  "contributors": [
    "Johannes Ziesmer <jhedtrich@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/universal/ref-napi.git"
  },
  "bugs": {
    "url": "https://github.com/universal/ref-napi/issues"
  },
  "homepage": "https://github.com/universal/ref-napi#readme",
  "main": "./lib/ref.js",
  "scripts": {
    "docs": "node docs/compile",
    "test": "nyc mocha --expose-gc",
    "install": "node-gyp-build",
    "prebuild-napi-x64": "prebuild -t 3 -r napi -a x64 --strip",
    "prebuild-napi-ia32": "prebuild -t 3 -r napi -a ia32 --strip",
    "upload": "prebuild --upload"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "get-symbol-from-current-process-h": "^1.0.2",
    "node-addon-api": "^3.0.0",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "dox": "0.9.0",
    "highlight.js": "^10.0.0",
    "jade": "^1.11.0",
    "marked": "^1.0.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prebuild": "^11.0.4",
    "weak-napi": "^2.0.0"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "config": {
    "runtime": "napi",
    "target": 3
  },
  "files": [
    "lib",
    "include",
    "docs",
    "binding.gyp"
  ]
}
